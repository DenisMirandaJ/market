generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum IngredientsEnum {
  tomato
  lemon
  potato
  rice
  ketchup
  lettuce
  onion
  cheese
  meat
  chicken
}

model BuyOrders {
  id            Int             @id @default(autoincrement())
  ingredient    IngredientsEnum @unique
  quantityToBuy Int

}
